{
  "name": "Cobalt2",
  "version": "1.0",
  "description": "A chrome theme adapted from Cobalt2 by Wes Bos",
  "manifest_version": 3,
  "theme": {
    "images": {
      "theme_ntp_background": "images/theme_ntp_background.png"
    },
    "colors": {
      "frame": [18, 39, 56],
      "toolbar": [25, 53, 73],
      "tab_text": [255, 198, 0],
      "tab_background_text": [165, 168, 169],
      "bookmark_text": [236, 236, 236],
      "ntp_background": [18, 39, 56],
      "ntp_text": [25, 53, 73],
      "ntp_link": [6, 55, 116],
      "button_background": [0, 0, 0, 0],
      "omnibox_background": [31, 70, 98, 0.95]
    },
    "tints": {
      "buttons": [0, 0, 0.93],
      "frame_inactive": [-1.0, -1.0, 0.55]
    },
    "properties": {
      "ntp_background_alignment": "left top",
      "ntp_background_repeat": "repeat"
    }
  }
}
